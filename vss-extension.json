{
    "manifestVersion": 1,
    "id": "<Custom-Task-Id>",
    "name": "<Friendly-Task-Name>",
    "publisher": "<verified-Publisher-Name>",
    "version": "0.0.0",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "<Task-Description-Here>",
    "categories": [
        "Build and release"
    ],
    "screenshots": [
        {
            "path": "screenshots/screenshot-0.png"
        },
        {
            "path": "screenshots/screenshot-0.png"
        },
        {
            "path": "screenshots/screenshot-0.png"
        }
    ],
    "galleryFlags": [
        "Public"
    ],
    "tags": [ "tag1", "tag2" ],
    "icons": {
        "default": "marketplace-icon.png"        
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/kasunkv/vsts-task-starter/wiki"
        },
        "support": {
            "uri": "https://github.com/kasunkv/vsts-task-starter/issues"
        },
        "repository": {
            "uri": "https://github.com/kasunkv/vsts-task-starter"
        },
        "issues": {
            "uri": "https://github.com/kasunkv/vsts-task-starter/issues"
        }
    },
    "branding": {
        "color": "rgb(74, 74, 74)",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/kasunkv/vsts-task-starter"
    },
    "files": [
        {
            "path": "dist/Tasks"
        },
        {
            "path": "screenshots",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "<Custom-Task-Id>",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "dist/Tasks/CustomTask"
            }
        }
    ],
    "CustomerQnASupport": {
        "enableqna":"true"
    } 
}